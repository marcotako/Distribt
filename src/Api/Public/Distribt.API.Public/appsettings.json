{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "OrderCluster",
        "Match": {
          "Path": "order-ms/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "OrderCluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7077/"
          }
        }
      }
    }
  }
}
